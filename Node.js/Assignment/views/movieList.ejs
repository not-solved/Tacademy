<html>
    <head>
        <meta charset="UTF-8" />
        <title>NodeJS Movie List</title>    
    </head>
    <body>
        <h1>Favorite Movies</h1>
        <div>
            <ul>
                <% data.forEach(function(movie) { %>
                <li><a href="/movies/<%= movie.movieId %>"><%= movie.title %></a></li>
                <% }); %>
            </ul>
        </div>

    </body>
</html>